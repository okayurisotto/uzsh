{
  "manifest_version": 2,
  "name": "uzsh",
  "description": "A minimal userscript & userstyle manager for Firefox.",
  "version": "1.0.0",
  "options_ui": {
    "page": "./dist-vite/options.html"
  },
  "background": {
    "scripts": ["./dist-tsup/index.global.js"]
  },
  "permissions": [
    "storage",
    "tabs",
    "webNavigation",
    "http://*/*",
    "https://*/*"
  ]
}
